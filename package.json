{
  "name": "wedding-frontend",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jsanter27/wedding-frontend.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check --ignore-path .gitignore .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write --ignore-path .gitignore .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@material-tailwind/react": "^2.0.1",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "color": "^4.2.3",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "next": "13.4.8",
    "next-intl": "^2.15.0-beta.5",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.8"
  }
}
